# URL de conexão do SQLAlchemy (PostgreSQL)
# Formato: postgresql://<USUARIO>:<SENHA>@<HOST>:<PORTA>/<DATABASE>
DATABASE_URL=postgresql://postgres:<YOUR_DB_PASSWORD>@db:5432/healthtrack

# Chave secreta usada para assinar JWTs
SECRET_KEY=<YOUR_SECRET_KEY>

# Algoritmo para JWT
ALGORITHM=HS256

# Tempo de expiração do token em minutos
ACCESS_TOKEN_EXPIRE_MINUTES=60
